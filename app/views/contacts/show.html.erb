<div class="row">
  <div class="col-md-12">
    <ol class="breadcrumb">
      <li><%=link_to "Dashboard", root_path %></a></li>
      <li><%=link_to "Contacts", contacts_path %></li>
      <li class="active">Show Contact</li>
    </ol>
  </div>
</div>

<%if notice %>
	<div class="alert alert-success" id="notice">
		<button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
		<span class="glyphicon glyphicon-ok-sign"></span> <%= notice %>
	</div>
<%end%>

  <p>
    <label>Name:</label>
    <%= @contact.name %>
  </p>
  <p>
    <label>Email:</label>
    <%= @contact.email %>
  </p>
  <p>
    <label>Key:</label>
    <%= @contact.key %>
  </p>
  <p>
    <label>Pages Accessed:</label>
  </p>
  <div class="table-responsive">
    <table class="table table-hover list">
      <thead>
        <tr>
          <th>URL</th>
          <th>Date and time</th>
        </tr>
      </thead>

      <tbody>
  <% @contact.accesses.order('created_at DESC').each do |access| %>
          <tr>
            <td><%= access.url %></td>
            <td><%= access.created_at %></td>
          </tr>
  <% end %>
      </tbody>
    </table>
  </div>

<%= link_to contacts_path, class: 'btn btn-primary' do %>
  <span class="glyphicon glyphicon-circle-arrow-left" aria-hidden="true"></span> Back
<%end%>
